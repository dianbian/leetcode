class Solution {
public:
    bool isPalindrome(int x) {
        if (x < 0) {
            return false;
        }
        string s = to_string(x);
        int m = 0;
        int n = s.size() - 1;
        while (m != n) {
            if (s[m] != s[n]) {
                return false;
            }
            m++;
            n--;
        }
        return true;
    }
};
