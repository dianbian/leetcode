class Solution {
public:
    int removeDuplicates(vector<int>& nums) {
        set<int> sets;
        for (int i = 0; i < nums.size(); ++i) {
            sets.insert(nums[i]);
        }
        int i = 0;
        for (auto& it : sets) {
            nums[i] = it;
            i++;
        }
        return sets.size();
    }
};
